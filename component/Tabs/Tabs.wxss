.tabs_container{
  
}
.tabs_title{
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction:row;
  height: 80rpx;
  border-bottom: var(--border-color) solid 1rpx;
}
.tabs_title_item{
  flex: 1;
  display: flex;
  height: 80rpx;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  color: var(--font-color);

}
.tabs_title_item_active{
  color: var(--themeColor);
  border-bottom: var(--theme-color) solid 1rpx;
}
 